def calculate_bmi(weight, height):
    BMI = weight/(height ** 2)
    return BMI

weight = float (input("Body Weight: "))
height = float(input("Body Height: "))

BMI=calculate_bmi(weight, height)

if (BMI<18.5):
    print ("Underweight")
elif (BMI<25):
    print ("Normal Weight")
elif (BMI<30):
    print ("Overweight")
else:
    print ("Obese")